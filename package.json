{
  "name": "sango",
  "version": "0.0.1-alpha",
  "description": "Academic data analysis platform",
  "main": "index.js",
  "scripts": {
    "start": "node api/index.js",
    "tscstart": "tsc && concurrently \"npm run tsc:w\" \"npm start\"",
    "insert": "node api/insert_mock_data.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "config": {
    "port": "3000",
    "db_url": "mongodb://localhost:27017/sango",
    "parser": "itaca",
    "upload_path": "/tmp/sango-uploads"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafarm/sango.git"
  },
  "keywords": [
    "analysis",
    "assessment",
    "student"
  ],
  "author": {
    "name": "Rafa Rubio",
    "email": "rubio_rafmor@gva.es"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rafarm/sango/issues"
  },
  "homepage": "https://github.com/rafarm/sango#readme",
  "dependencies": {
    "@angular/common": "~2.4.1",
    "@angular/compiler": "~2.4.1",
    "@angular/core": "~2.4.1",
    "@angular/forms": "~2.4.1",
    "@angular/http": "~2.4.1",
    "@angular/platform-browser": "~2.4.1",
    "@angular/platform-browser-dynamic": "~2.4.1",
    "@angular/router": "~3.4.1",
    "@angular/upgrade": "~2.4.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "mongodb": "^2.2.10",
    "multer": "^1.2.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.1",
    "systemjs": "0.19.40",
    "xmldoc": "^0.5.1",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "typescript": "~2.0.10",
    "@types/node": "^6.0.46"
