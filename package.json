{
  "name": "sango",
  "version": "0.0.1-alpha",
  "description": "Academic data analysis platform",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "node api/index.js",
    "build": "ng build --prod"
  },
  "config": {
    "port": "443",
    "key": "/etc/ssl/private/ssl-cert-snakeoil.key",
    "cert": "/etc/ssl/certs/ssl-cert-snakeoil.pem",
    "db_url": "mongodb://localhost:27017/sango",
    "parser": "itaca",
    "upload_path": "/tmp/sango-uploads",
    "locale": "ca",
    "ldap_url": "ldap://localhost:389",
    "ldap_bindDN": "cn=admin,dc=sango,dc=org",
    "ldap_bindCredentials": "adminldap",
    "ldap_searchBase": "ou=users,dc=sango,dc=org",
    "ldap_searchFilter": "(uid={{username}})",
    "cookie_max_age": "3600000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafarm/sango.git"
  },
  "keywords": [
    "analysis",
    "assessment",
    "student"
  ],
  "author": {
    "name": "Rafa Rubio",
    "email": "rubio_rafmor@gva.es"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rafarm/sango/issues"
  },
  "homepage": "https://github.com/rafarm/sango#readme",
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.6",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^2.0.0",
    "connect-mongodb-session": "^1.4.0",
    "core-js": "^2.4.1",
    "ejs": "^2.5.7",
    "express": "^4.14.0",
    "express-session": "^1.15.6",
    "mongodb": "^2.2.10",
    "multer": "^1.2.0",
    "passport": "^0.4.0",
    "passport-ldapauth": "^2.0.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.5.2",
    "systemjs": "^0.19.47",
    "xmldoc": "^0.5.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.3",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "ts-node": "~3.2.0",
    "typescript": "~2.4.2"
  }
}
