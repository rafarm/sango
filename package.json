  "description": "Academic data analysis platform",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "node api/index.js",
    "build": "ng build --prod"
  },
  "config": {
    "port": "4443",
    "key": "/etc/ssl/intermediate/private/key.pem",
