{
  "name": "sango",
  "version": "0.0.1-alpha",
  "description": "Academic data analysis platform",
  "main": "index.js",
  "scripts": {
    "start": "node api/index.js",
    "tscstart": "tsc && concurrently \"npm run tsc:w\" \"npm start\"",
    "insert": "node api/insert_mock_data.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "config": {
    "port": "3000",
    "db_url": "mongodb://localhost:27017/sango",
    "parser": "itaca",
    "upload_path": "/tmp/sango-uploads",
    "locale": "ca",
    "ldap_url": "ldap://localhost:389",
    "ldap_bindDN": "cn=admin,dc=sango,dc=org",
    "ldap_bindCredentials": "adminldap",
    "ldap_searchBase": "ou=users,dc=sango,dc=org",
    "ldap_searchFilter": "(uid={{username}})",
    "cookie_max_age": "3600000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafarm/sango.git"
  },
  "keywords": [
    "analysis",
    "assessment",
    "student"
  ],
  "author": {
    "name": "Rafa Rubio",
    "email": "rubio_rafmor@gva.es"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rafarm/sango/issues"
  },
  "homepage": "https://github.com/rafarm/sango#readme",
  "dependencies": {
    "@angular/common": "^4.0.2",
    "@angular/compiler": "^4.0.2",
    "@angular/core": "^4.0.2",
    "@angular/forms": "^4.0.2",
    "@angular/http": "^4.0.2",
    "@angular/platform-browser": "^4.0.2",
    "@angular/platform-browser-dynamic": "^4.0.2",
    "@angular/router": "^4.0.2",
    "@angular/upgrade": "^4.0.2",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.6",
    "connect-mongo": "^2.0.0",
    "connect-mongodb-session": "^1.4.0",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "express-session": "^1.15.6",
    "mongodb": "^2.2.10",
    "multer": "^1.2.0",
    "passport": "^0.4.0",
    "passport-ldapauth": "^2.0.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.1",
    "systemjs": "^0.19.47",
    "xmldoc": "^0.5.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/node": "^6.0.69",
    "concurrently": "^3.1.0",
    "typescript": "^2.3.0"
  }
}
