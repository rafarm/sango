{
    "name": "sango",
    "version": "0.0.1-alpha",
    "description": "Academic data analysis platform",
    "main": "index.js",
    "scripts": {
    	"start": "node api/index.js",
    	"tscstart": "tsc && concurrently \"npm run tsc:w\" \"npm start\"",
    	"insert": "node api/insert_mock_data.js",
    	"postinstall": "typings install",
    	"tsc": "tsc",
    	"tsc:w": "tsc -w",
    	"typings": "typings"
    },
    "config": {
    	"port": "3000",
    	"db_url": "mongodb://localhost:27017/sango"
    },
    "repository": {
    	"type": "git",
    	"url": "git+https://github.com/rafarm/sango.git"
    },
    "keywords": [
    	"analysis",
    	"assessment",
    	"student"
    ],
    "author": {
    	"name": "Rafa Rubio",
    	"email": "rubio_rafmor@gva.es"
    },
    "license": "MIT",
    "bugs": {
    	"url": "https://github.com/rafarm/sango/issues"
    },
    "homepage": "https://github.com/rafarm/sango#readme",
    "dependencies": {
    	"@angular/common": "2.0.0",
    	"@angular/compiler": "2.0.0",
    	"@angular/core": "2.0.0",
    	"@angular/forms": "2.0.0",
    	"@angular/http": "2.0.0",
    	"@angular/platform-browser": "2.0.0",
    	"@angular/platform-browser-dynamic": "2.0.0",
    	"@angular/router": "3.0.0",
    	"core-js": "^2.4.1",
    	"reflect-metadata": "^0.1.3",
    	"rxjs": "5.0.0-beta.12",
    	"systemjs": "0.19.27",
    	"zone.js": "^0.6.23",
    	"@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.2",
    	"bootstrap": "^4.0.0-alpha.4",
    	"body-parser": "^1.15.2",
    	"express": "^4.14.0",
    	"mongodb": "^2.2.10"
    },
    "devDependencies": {
    	"concurrently": "^2.0.0",
    	"typescript": "^1.8.10",
    	"typings": "^1.0.4"
    }
}
